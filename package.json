    "build": "./node_modules/.bin/lerna exec -- yarn build",
    "serve": "./node_modules/.bin/storybook dev",
    "storybook-build": "./node_modules/.bin/storybook build",
    "generate-types": "./node_modules/.bin/lerna exec -- npm run generate-types"
  },
